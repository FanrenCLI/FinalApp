  .card-box{
    background-image: url("https://fanrencli.cn/1.png");
  }
  .line_1{
    position: absolute;
    z-index: -1;
    width: 100%;
    opacity: 0.7;
  }
  .line_2{
    
    opacity: 0.4;
    height: 104rpx;
    border-bottom: 1px dashed #6e6e6e;
  }
  
  .flex-item {
    width: 110rpx;
    height: 80rpx;
  }
  
  .kcb-item {
    position: absolute;
    justify-content: center;
    display: flex;
    border-radius: 5px;
    overflow: hidden;
    
  }
  .smalltext {
    font-size: 8pt;
    color: #333;
    padding-left: 2px;
    line-height: 40rpx;
  }
  
  .top {
